{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "eslint": "eslint '**/*.js'",
    "prettier": "prettier --write '**/*.js'",
    "pretest": "prettier --list-different '**/*.js'",
    "prestorybook": "rnstl && prettier --write .storybook/storyLoader.js",
    "storybook": "expo start --config app.story.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretest && npm run eslint"
    }
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./.storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.9.1",
    "@react-navigation/native": "^5.7.5",
    "expo": "^39.0.0",
    "expo-status-bar": "~1.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.0.tar.gz",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-paper": "^4.2.0",
    "react-native-reanimated": "~1.13.0",
    "react-native-safe-area-context": "3.1.4",
    "react-native-screens": "~2.10.1",
    "react-native-web": "~0.13.7",
    "styled-components": "^5.2.0",
    "@react-native-community/async-storage": "~1.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@storybook/react-native": "^5.3.21",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-native": "^7.0.2",
    "@twostoryrobot/eslint-config": "^2.2.2",
    "@twostoryrobot/prettier-config": "^2.0.1",
    "babel-loader": "^8.1.0",
    "babel-preset-expo": "^8.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-native": "^3.9.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-expo": "^39.0.0",
    "prettier": "^2.1.2",
    "react-is": "^16.13.1",
    "react-native-storybook-loader": "^2.0.2",
    "react-test-renderer": "^16.13.1"
  },
  "private": true
}
